cdfa {
    start
        ' ' | '\t' | '\r' -> start
        '\n' -> ^NEWLINE
        _ -> line;

    line ^LINE
        ' ' | '\t' | '\r' -> after_line
        '\n' -> fail
        _ -> line;

    after_line
        ' ' | '\t' | '\r' -> after_line
        '\n' -> fail
        _ -> line;
}

grammar {
    file
        | {line}
        |;

    line
        | LINE
        | NEWLINE `\n`;
}
